//    get-property className | equals "org.eclipse.nebula.widgets.nattable.NatTable" | verify-true
    get-property "getRowCount()" | equals 10 | verify-true
}
with [get-editor "test-corpus.salt" | get-canvas] {
	key-type "M1+s"
 	key-type "M1+w"
}
