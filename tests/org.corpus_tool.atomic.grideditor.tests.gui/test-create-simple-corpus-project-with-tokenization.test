Entry-Name: .content

get-view Navigation | get-tree | get-menu -path "Import..." | click
with [get-window Import] {
    get-tree | select "Atomic/Corpus Import"
    get-button "Next >" | click
}
wait 500
with [get-window "Import via Pepper"] {
    get-table | select TextImporter
    get-button "Next >" | click
    get-table | select txt
    get-button "Next >" | click
    get-button File | click
}
wait 500
set-dialog-result File "/home/travis/build/infraling/atomic/tests/org.corpus_tool.atomic.grideditor.tests.gui/src/test/resources/test-corpus.txt"
wait 500
with [get-window "Import via Pepper"] {
    get-button "..." | click
    get-button "Next >" | click
    get-table | select "pepper.after.tokenize"
}
wait 500
with [get-window "Import via Pepper" | get-table] {
    select "pepper.after.tokenize"
    get-cell 5 1 | activate-cell-edit | check
}
wait 500
get-window "Import via Pepper" | get-table | select "pepper.before.addSLayer"
with [get-window "Import via Pepper" | get-table] {
    get-item -path "pepper.after.tokenize" | get-property "columns[1]" | equals true | verify-true
    get-item -path "pepper.after.tokenize" | get-property "values['Value']" | equals true | verify-true
    get-item -path "pepper.after.tokenize" | get-property "getData().getValue().toString()" | equals true | verify-true
}
wait 500
with [get-window "Import via Pepper"] {
    get-button "Next >" | click
    get-editbox -after [get-label "Project name; must be unique"] | set-text "simple-corpus"
    get-button Finish | click
}
wait 5000
with [get-view Navigation | get-tree] {
    select "simple-corpus/test-corpus/test-corpus.salt" | key-type F5
    select "simple-corpus/test-corpus/test-corpus.salt" | get-menu -path "Open With/Text Editor" | click
}
get-editor "test-corpus.salt" | get-text-viewer | get-property text | contains "<labels xsi:type=\"saltCore:SFeature\" namespace=\"saltCommon\" name=\"SDATA\" value=\"T::Is this example more complicated than it appears to be?\"/>\n" | verify-true
with [get-editor "test-corpus.salt" | get-text-viewer] {
    key-type "M1+w"
}
