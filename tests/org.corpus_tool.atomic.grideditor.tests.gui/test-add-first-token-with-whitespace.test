Content-Type: text/ecl
Entry-Name: .content

get-view Navigation | get-tree | select "simple-corpus/test-corpus/test-corpus.salt" | get-menu 
    -path "Open With/Grid Editor" | click
// Wait for grid to compile
wait 5000
with [get-editor "test-corpus.salt" | get-canvas] {
    get-property className | equals "org.eclipse.nebula.widgets.nattable.NatTable" | verify-true
    get-property "getRowCount()" | equals 12 | verify-true
}
with [get-editor "test-corpus.salt" | get-canvas] {
	mouse-press 150 30 button3
	mouse-release 150 30 button3
	get-menu "Create new first token" | click
}
with [get-window -class TokenTextInputDialog] {
    get-editbox -after [get-label "Please enter the text which the new token should cover."] | set-text First
    wait 1000
    get-button OK | click
}
wait 1000
with [get-editor "test-corpus.salt" | get-canvas] {
    get-property className | equals "org.eclipse.nebula.widgets.nattable.NatTable" | verify-true
    get-property "getRowCount()" | equals 13 | verify-true
}
with [get-editor "test-corpus.salt" | get-canvas] {
    key-type "M1+s"
    key-type "M1+w"
}
get-view Navigation | get-tree | select "simple-corpus/test-corpus/test-corpus.salt" | get-menu 
    -path "Open With/Grid Editor" | click
with [get-editor "test-corpus.salt" | get-canvas] {
    get-property className | equals "org.eclipse.nebula.widgets.nattable.NatTable" | verify-true
    get-property "getRowCount()" | equals 13 | verify-true
}
get-editor "test-corpus.salt" | get-canvas | key-type "M1+w"
with [get-view Navigation | get-tree] {
    select "simple-corpus/test-corpus/test-corpus.salt" | key-type F5
    select "simple-corpus/test-corpus/test-corpus.salt" | get-menu -path "Open With/Text Editor" | click
}
get-editor "test-corpus.salt" | get-text-viewer | get-property text | contains "<labels xsi:type=\"saltCore:SFeature\" namespace=\"saltCommon\" name=\"SDATA\" value=\"T::First Is this example more complicated than it appears to be?\"/>\n" | verify-true
get-editor "test-corpus.salt" | get-text-viewer | key-type "M1+w"
