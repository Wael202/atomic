<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="de.uni_jena.iaa.linktype.atomic.application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="de.uni_jena.iaa.linktype.atomic.Atomic">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uni_jena.iaa.linktype.atomic.application"
            name="Atomic (mixed mode)">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>

</plugin>
